{
  "name": "testing",
  "version": "1.0.0",
  "main": "cypress.config.js",
  "type": "module",
"scripts": {
    "clean-reports": "rm -rf cypress/reports/*",
    "cypress:run": "npm run clean-reports && cypress run --reporter mochawesome --reporter-options reportDir=cypress/reports,overwrite=false,html=true,json=true",
    "merge-reports": "mochawesome-merge cypress/reports/*.json > cypress/reports/mochawesome_merged.json",
    "generate-report": "marge cypress/reports/mochawesome_merged.json --reportDir cypress/reports --inline",
    "open-report": "open cypress/reports/mochawesome_merged.html",
    "test": "npm run cypress:run && npm run merge-reports && npm run generate-report && npm run open-report"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^6.0.2",
    "cypress": "^13.15.0",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0",
    "@badeball/cypress-cucumber-preprocessor": "^21.0.2",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.3"
  }
}


